{
  "name": "cvs-extension",
  "version": "1.0.0",
  "main": "src/extension.js",
  "publisher": "Hanna",
  "engines": {
    "vscode": "^1.22.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:cvs.formatDocument"
  ],
  "contributes": {
    "commands": [
      {
        "command": "cvs.formatDocument",
        "title": "Format current document with Prettier"
      }
    ],
    "snippets": [
      {
        "path": "src/snippets.code-snippets"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "crt-container",
          "title": "CRT Dev Tools",
          "icon": "resources/heart.svg"
        }
      ]
    },
    "views": {
      "crt-container": [
        {
          "id": "crt-web",
          "name": "CRT Dev Tools",
          "type": "webview"
        }
      ],
      "explorer": [
        {
          "id": "crt-explorer",
          "name": "CRT Dev Tools",
          "type": "webview"
        }
      ]
    }
  },
  "dependencies": {
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0"
  }
}
